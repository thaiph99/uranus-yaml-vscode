{
  "name": "uranus-yaml",
  "displayName": "Uranus YAML - Argo Workflow Navigator",
  "description": "Intelligent Ctrl+Click navigation for Argo WorkflowTemplate references in YAML files. Navigate seamlessly between template definitions and usages.",
  "version": "0.0.1",
  "publisher": "ThaiPham",
  "author": {
    "name": "Thai Pham"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaiph99/uranus-yaml-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/thaiph99/uranus-yaml-vscode/issues"
  },
  "homepage": "https://github.com/thaiph99/uranus-yaml-vscode#readme",
  "keywords": [
    "yaml",
    "argo",
    "workflow",
    "template",
    "navigation",
    "go-to-definition",
    "kubernetes",
    "devops"
  ],
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "yaml",
        "aliases": [
          "YAML"
        ],
        "extensions": [
          ".yaml",
          ".yml"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}